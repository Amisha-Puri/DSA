#include <iostream>
using namespace std;

int main() {

    int hash[256] = {0};   // ASCII size

    string s = "hello";

    for(int i = 0; i < s.length(); i++) {
        hash[s[i]]++;   // ASCII value used as index
    }

    // Print frequency
    for(int i = 0; i < 256; i++) {
        if(hash[i] > 0)
            cout << char(i) << " -> " << hash[i] << endl;
    }

    return 0;
}
